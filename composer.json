{
    "name": "guanguans/laupload",
    "description": "一个 laravel 上传小组件",
    "type": "library",
    "keywords": [
        "laravel",
        "upload",
        "laupload"
    ],
    "homepage": "https://github.com/guanguans/laupload",
    "require": {
        "php": ">=5.6.0",
        "npm-asset/hdjs": "~1.2"
    },
    "require-dev": {
        "php": ">=5.6.0",
        "phpunit/phpunit": "~5 || ~7",
        "mockery/mockery": "~1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "guanguans",
            "email": "798314049@qq.com"
        }
    ],
    "config": {
      "process-timeout": 1800,
      "fxp-asset": {
        "enabled": false
      }
    },
    "autoload": {
        "psr-4": {
            "guanguans\\laupload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\Tests\\": "tests/"
        }
    },
    "scripts": {
      "fxp": "composer global require fxp/composer-asset-plugin"
    }
}
